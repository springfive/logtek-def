        <fullName>DesignationYear__c</fullName>
        <description>Anniversary year of the PM Plan.</description>
        <externalId>false</externalId>
        <formula>if(CurrentDate__c &gt;= SVMXC__Start_Date__c,
TEXT(YEAR(CurrentDate__c - PM_Delta_Start_of_Year__c)),
TEXT(YEAR(SVMXC__Start_Date__c))
)</formula>
        <label>Designation Year</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>NextAnniversaryDate__c</fullName>
        <externalId>false</externalId>
        <formula>DATE(
YEAR (SVMXC__Start_Date__c) + (FLOOR((CurrentDate__c - SVMXC__Start_Date__c) / 365) + 1),
MONTH(SVMXC__Start_Date__c),
IF(AND(DAY(SVMXC__Start_Date__c) = 29,MONTH(SVMXC__Start_Date__c) = 02) , 28, DAY(SVMXC__Start_Date__c)))</formula>
        <label>Next Anniversary Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
