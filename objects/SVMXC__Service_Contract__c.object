    <validationRules>
        <fullName>DEF_WW_Make_Contract_As_ReadOnly</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    CONTAINS($Label.DEF_Contract_Validation_Profile,$Profile.Name), 
TEXT(PRIORVALUE(Status__c)) = &apos;Active&apos;, 
OR(ISCHANGED(SVMX_PS_Parent_Contract__c),
ISCHANGED(Name),
ISCHANGED(SVMXC__Active__c),
ISCHANGED(Status__c),
ISCHANGED(SVMXC__Company__c),
ISCHANGED(SVMXC__Contact__c),
ISCHANGED(SVMXC__Service_Contract_Notes__c),
ISCHANGED(Location__c),
ISCHANGED(SVMXC__Start_Date__c),
ISCHANGED(SVMXC__End_Date__c),
ISCHANGED(Installation__c),
ISCHANGED(SVMXC__Service_Plan__c),
ISCHANGED(Preferred_Technician__c),
ISCHANGED(SVMXC__Sales_Rep__c),
ISCHANGED(Description__c),
ISCHANGED(Signature_Date__c),
ISCHANGED(Bill_to__c),
ISCHANGED(Is_Evergreen__c),
ISCHANGED(SVMXC__Renewal_Number__c),
ISCHANGED(SVMX_PS_Renewed__c),
ISCHANGED(SVMXC__Renewal_Date__c),
ISCHANGED(SVMXC__Renewed_From__c),
ISCHANGED(Renewal_Price_Increase_Percentage__c),
ISCHANGED(Date_of_Notice__c),
ISCHANGED(Reason_for_Cancellation__c),
ISCHANGED(SVMXC__Canceled_By__c),
ISCHANGED(SVMXC__Canceled_On__c),
ISCHANGED(OwnerId)
)
)</errorConditionFormula>
        <errorMessage>Contract cannot be modified. Please contact System Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__Service_Contract_Future_End_Date</fullName>
        <active>false</active>
        <description>Service contract end date should be in the future</description>
        <errorConditionFormula>AND(ISNEW(),  SVMXC__End_Date__c  &lt;  TODAY())</errorConditionFormula>
        <errorDisplayField>SVMXC__End_Date__c</errorDisplayField>
        <errorMessage>End date should be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__Service_Contract_Start_End_Date</fullName>
        <active>true</active>
        <description>Service contract should have valid start and end dates</description>
        <errorConditionFormula>SVMXC__End_Date__c  &lt;  SVMXC__Start_Date__c</errorConditionFormula>
        <errorDisplayField>SVMXC__End_Date__c</errorDisplayField>
        <errorMessage>End date should be greater or equal to Start date.</errorMessage>
    </validationRules>
