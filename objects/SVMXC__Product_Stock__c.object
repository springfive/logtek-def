    <fields>
        <fullName>SVMX_PS_Reorder__c</fullName>
        <description>Indicates if a reorder is required</description>
        <externalId>false</externalId>
        <formula>IF(
AND(
NOT(ISBLANK(  SVMXC__Reorder_Level2__c  )), 
SVMXC__Reorder_Level2__c &gt; 0, 
SVMXC__Available_Qty__c &lt;=  SVMXC__Reorder_Level2__c),
true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Reorder?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
