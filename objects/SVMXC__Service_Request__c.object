        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Time_Elapsed__c</fullName>
        <externalId>false</externalId>
        <formula>/* If SR is closed or converted to a Work Order then 
we calculate the time between the SR created Date and the Date of closeure
else we calculate the time between the created date and the current Date*/
IF(ISPICKVAL(SVMXC__Status__c, &apos;Closed&apos;)||ISPICKVAL(SVMXC__Status__c, &apos;Open_Converted&apos;), 
			(SVMXC__Actual_Initial_Response__c - CreatedDate)*24, 
			(Now() - CreatedDate)*24)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Time Elapsed</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
