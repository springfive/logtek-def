    <fields>
        <fullName>Confirmation_Request_Status__c</fullName>
        <externalId>false</externalId>
        <label>Confirmation Request Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Request Not Sent</fullName>
                    <default>false</default>
                    <label>Request Not Sent</label>
                </value>
                <value>
                    <fullName>Request Sent</fullName>
                    <default>false</default>
                    <label>Request Sent 📧❓</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Designation__c</fullName>
        <externalId>false</externalId>
        <formula>/*Confirmation request*/
IF(ISPICKVAL (Confirmation_Request_Status__c, &quot;Request Sent&quot;)&amp;&amp; NOT(ISPICKVAL(SVMXC__Order_Status__c, &quot;Customer Confirmed&quot;)) ,&quot;❓&quot;&amp;&quot; | &quot;,&quot;&quot;)

/*Customer confirmed*/
&amp;
IF( ISPICKVAL (SVMXC__Order_Status__c, &quot;Customer Confirmed&quot;), 
&quot;✅&quot;&amp;&quot; | &quot;,&quot;&quot;)

/*Order Type*/
&amp;
IF(ISPICKVAL( SVMXC__Order_Type__c, &quot;Maintenance&quot;),
			&quot;🛠️ &quot;,
			IF(ISPICKVAL(SVMXC__Order_Type__c, &quot;Outcalls&quot;), 
		    	&quot;☎️ &quot;,
					  IF(ISPICKVAL(SVMXC__Order_Type__c, &quot;New Works&quot;), 
		    	&quot;📐 &quot;,&quot;🔧&quot;)))

/*Installation discipline*/
&amp;
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0001&quot;),&quot;| &quot;&amp;&quot;🔥&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0010&quot;),&quot;| &quot;&amp;&quot;📢&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0060&quot;),&quot;| &quot;&amp;&quot;💻&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0070&quot;),&quot;| &quot;&amp;&quot;🔒&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0080&quot;),&quot;| &quot;&amp;&quot;🛑&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0020&quot;),&quot;| &quot;&amp;&quot;🧯🌀&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0030&quot;),&quot;| &quot;&amp;&quot;🧯❄️&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0040&quot;),&quot;| &quot;&amp;&quot;🧯💨&quot;,
IF( ISPICKVAL(Installation_Discipline__c, &quot;ID0050&quot;),&quot;| &quot;&amp;&quot;🧯💦&quot;,&quot;&quot;)))))))))

	/*Location*/
	&amp;&quot;| &quot;&amp;
	SVMXC__Site__r.Name</formula>
        <label>Designation</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Scheduled_End_Date_Time__c</fullName>
        <externalId>false</externalId>
        <formula>SVMXC__Scheduled_Date_Time__c+(Scheduled_Duration_in_hours__c/24)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Scheduled End Date Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
